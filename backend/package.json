    "nodemon": "^3.1.0",
    "sequelize-cli": "6.6.1"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "eslint-config-trybe-backend": "^2.1.0",
    "express": "^4.18.3",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.9.2",
    "sequelize": "^6.37.1",
    "yargs": "^17.7.2"
  }
}
